{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node - Launch File",
      "program": "${file}",
      "useWSL": true
    },
    {
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "name": "Chrome - localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    }, {
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "name": "Chrome - Polymer Serve",
      "url": "127.0.0.1:8081",
      "webRoot": "${workspaceFolder}"
    }
  ]
}