{
  
  "entrypoint": "index.html",
  
  "shell": "src/sp-shell.html",
  
  "fragments": [
    "src/sp-upload.html",
    "src/sp-console.html",
    "src/notifier.html"
  ],
  
  "sources": [
   "src/**/*",
   "images/**/*",
   "bower.json"
  ],
  
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js",
    "loader.css",
    "images/icons/48.png",
    "images/icons/144.png",
    "images/icons/192.png",
    "images/icons/svg.svg",
    "images/upload.svg",
    "favicon.ico"
  ],
  
  "lint": {
    "rules": [ "polymer-2" ]
  },
  
  "builds": [
    {
      "name": "production",
      "preset": "es6-bundled",
      "swPrecacheConfig": "sw-precache-config.js"
    }
  ]
  
}
